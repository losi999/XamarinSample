<Application
    x:Class="XamarinSample.Windows81.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="using:XamarinSample.ViewModel"
    xmlns:converters="using:XamarinSample.Windows81.Converters"
    xmlns:local="using:XamarinSample.Windows81">
    <Application.Resources>
        <vm:ViewModelLocator
            x:Name="Locator" />
        <converters:StringToVisibilityConverter
            x:Name="StringToVisibilityConverter" />
        <converters:BooleanToInverseBooleanConverter
            x:Name="BooleanToInverseBooleanConverter" />
    </Application.Resources>
</Application>
